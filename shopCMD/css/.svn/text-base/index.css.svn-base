.show-case{

}
.show-case a{
	display: block;
	text-decoration: none;
	overflow: hidden;
	width: 324px;
	height:210px;
	position: relative;
}
.show-case a img {
	height: 210px;
	width: 324px;
	transition: all .6s ease 0s;
	-ms-transition: all .6s ease 0s;
	-moz-transition: all .6s ease 0s;
	-webkit-transition: all .6s ease 0s;
}
.show-case a:hover img{
	transform:scale(1.1);
	-ms-transform:scale(1.1);
	-moz-transform:scale(1.1);
	-webkit-transform:scale(1.1)
}
.show-case a:hover span{
	display: block;
	transition: 1s all ease;
}
.show-case a h4{
	background-color: rgba(0,0,0,.3);
	color: #fff;
	font-size: 14px;
	font-weight: 500;
	height: 46px;
	line-height: 46px;
	width: 324px;
	position: absolute;
	left: 0;
	bottom: 0;
}
.show-case a span{
	background-color: rgba(0,0,0,.3);
	font-size: 14px;
	line-height: 46px;
	width: 324px;
	position: absolute;
	left: 0;
	bottom: 0;
	height:210px;
	display: none;
}
.show-case a span i{
	background: url(../images/ser.png) no-repeat;
	height: 72px;
	width: 72px;
	position: absolute;
	left: 50%;
	top: 50%;
	margin: -28px 0 0 -36px;
}

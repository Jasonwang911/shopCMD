.banner{
  width:100%;
  height:319px;
  img{
    width:100%;
    height:100%;}
}
.right{
  position: fixed;
  right:12px;
  top:380px;
  a{
    display: block;
    width:50px;
    height:50px; font-size: 12px;  border-radius: 4px; margin-bottom: 10px; text-align: center;
    &:nth-child(1){
      background: url("../img/question.png") #7c7c7c no-repeat 7px 9px;
    }
    &:nth-child(2){
      background: url("../img/question.png") #7c7c7c no-repeat 7px -34px;
    }

  }
}
.content{padding-top: 59px;
  .cont{
    width:1198px;
    margin:0 auto;
    .list{
      float: left; margin-right: 80px;
      a{display: block;
        height:26px;
        color: #666666; font-size: 16px; margin-bottom: 38px; padding-left: 20px;
        border-left:3px solid #fff; line-height: 26px;}
      a:hover{
        color:#16a0fc;
        border-left:3px solid #16a0fc;}
      .active{
        color:#16a0fc;
        border-left:3px solid #16a0fc;}

    }
    .cont_list{
      float: right;
      width:999px;
      li{ display: none;
        h2{
          font-size: 16px;
          font-weight:bold;height: 38px; border-bottom: 1px solid #f3f3f3;
        }
        p{ font-size: 12px;
          color: #333333; line-height: 24px; margin-top: 14px; margin-bottom: 15px;}
        a{display: block; font-size: 16px;height: 37px;
          color: #333; font-weight: bold; border-bottom: 1px solid #f3f3f3; margin-bottom: 20px}
      }
     .show{display: block;}
    }
  }
}

